    <style name="title_bar_style">
        <item name="android:background">@drawable/bg_titlebar</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">11dp</item>
    </style>

    <style name="top_update_count">
        <item name="android:textSize">10.0sp</item>
        <item name="android:textColor">#ffcc3131</item>
        <item name="android:gravity">center</item>
        <item name="android:height">15.0dip</item>
        <item name="android:width">15.0dip</item>
    </style>

 
      <style name="normal_Labletextstyle">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/white2</item>
    </style>
        
    <style name="subscribe_my_tip_text">
        <item name="android:textSize">16.0sp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">@color/subscribe_tip_text</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>
        
    
    <style name="subscribe_seperate_line">
        <item name="android:background">@color/subscribe_seperate_line</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">0.5dip</item>
        <item name="android:layout_marginTop">14.0dip</item>
        <item name="android:layout_marginBottom">14.0dip</item>
    </style>
        
     <style name="text_on_button">
        <item name="android:textSize">15.0sp</item>
        <item name="android:textColor">#ffffffff</item>
        <item name="android:shadowRadius">0.0</item>
    </style>
    
   <style name="normal_textstyle">
        <item name="android:textColor">@color/textcolor</item>
        <item name="android:textSize">@dimen/f21</item>
    </style>

    <style name="screen_margin_style">
        <item name="android:layout_marginRight">@dimen/pad3</item>
        <item name="android:layout_marginTop">@dimen/pad3</item>
        <item name="android:layout_marginLeft">@dimen/pad3</item>
    </style>

    <style name="navigation_radiobutton_style">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:button">@null</item>
        <item name="android:paddingTop">@dimen/pad1</item>
        <item name="android:paddingBottom">@dimen/pad1</item>
        <item name="android:layout_weight">1</item>
        <item name="android:textColor">@color/white2</item>
        <item name="android:gravity">center</item>
        <item name="android:textSize">@dimen/f18</item>
        <item name="android:background">@drawable/radiolbutton_background_selector</item>
    </style>

    <style name="blue_buttom_style">
        <item name="android:textColor">@color/white2</item>
        <item name="android:background">@drawable/blue_button_background_selector</item>
        <item name="android:textSize">@dimen/f21</item>
    </style>

    <style name="green_buttom_style">
        <item name="android:textColor">@color/white2</item>
        <item name="android:textSize">@dimen/f21</item>
    </style>

    <style name="custom_dialog" parent="@android:style/Theme.Dialog">

        <!-- 边框 -->
        <item name="android:windowFrame">@null</item>
        <!-- 是否浮现在activity之上 -->
        <item name="android:windowIsFloating">true</item>
        <!-- 屏幕背景变暗 -->
        <item name="android:backgroundDimEnabled">true</item>
        <!-- 半透明 -->
        <item name="android:windowIsTranslucent">false</item>
        <!-- 无标题 -->
        <item name="android:windowNoTitle">true</item>
        <!-- 背景透明 -->
        <item name="android:windowBackground">@android:color/transparent</item>
    </style>
  

   
</resources>